    "email-validator": "^2.0.4",
    "he": "^1.2.0",
    "lodash.chunk": "^4.2.0",
    "lodash.omit": "^4.5.0",
    "lru_map": "^0.4.1",
    "mobx": "^6.6.1",
    "mobx-react-lite": "^3.4.0",
    "react": "18.2.0",
    "react-avatar-editor": "^13.0.0",
    "react-circular-progressbar": "^2.1.0",
    "react-dom": "^18.2.0",
    "react-native": "0.71.0",
    "react-native-appstate-hook": "^1.0.6",
